{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "dnsrecord": "touch docs/CNAME && echo 'www.kasana.io' > docs/CNAME",
    "nojekyll": "touch docs/.nojekyll",
    "export": "next build && next export -o docs",
    "fullexport": "yarn export && yarn dnsrecord && yarn nojekyll",
    "start": "NODE_ENV=production PORT=$PORT node server.js",
    "lint": "next lint",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "next": "12",
    "next-routes": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3"
  }
}
